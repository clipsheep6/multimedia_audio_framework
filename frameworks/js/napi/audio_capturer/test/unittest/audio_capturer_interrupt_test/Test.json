{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "ohos.audiocapture.bundle.stage.test",
        "module-name": "entry",
        "shell-timeout": "600000",
        "testcase-timeout": 70000
    },
    "kits": [
        {
            "test-file-name": [
                "AudioCapturerInterruptJsUnitTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "param set persist.ace.testmode.enabled 1",
                "power-shell wakeup",
                "uinput -T -d 300 600 -m 300 600 300 100 -u 300 100",
                "power-shell setmode 602"
            ],
            "teardown-command":[
                "bm uninstall -n ohos.audiocapture.bundle.stage.test",
                "power-shell setmode 600"
            ]
        }
    ]
}

